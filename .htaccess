RewriteRule ^conteudo/([A-Za-z0-9-]+)/([A-Za-z0-9-]+)/?$ conteudo/index.php?p=$1&r=$2 [L]

RewriteRule ^sistema/conteudo/([A-Za-z0-9-]+)/?$ sistema/conteudo/index.php?p=$1[L]

